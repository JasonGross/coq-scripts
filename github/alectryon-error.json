{
    "problemMatcher": [
        {
            "owner": "alectryon-problem-matcher-!!-error",
            "pattern": [
                {
                    "regexp": "^!! (Error):\\s*(.*)$",
                    "severity": 1,
                    "message": 2
                }
            ]
        },
        {
            "owner": "alectryon-problem-matcher-error",
            "pattern": [
                {
                    "regexp": "^([^:]+):(\\d+):\\s*\\((ERROR)/(\\d+)\\)\\s+(.*)$",
                    "file": 1,
                    "line": 2,
                    "severity": 3,
                    "code": 4,
                    "message": 5
                }
            ]
        }
    ]
}
