ifdef VDFILE
# Coq >= 8.8
$(VDFILE).d: | $(COMPATIBILITY_FILE)
endif
$(VFILES:.v=.v.d): | $(COMPATIBILITY_FILE)
$(ML4FILES:.ml4=.ml4.d): | $(COMPATIBILITY_FILE) $(ML_COMPATIBILITY_FILES)
$(MLFILES:.ml=.ml.d): | $(COMPATIBILITY_FILE) $(ML_COMPATIBILITY_FILES)
$(MLLIBFILES:.mllib=.mllib.d): | $(COMPATIBILITY_FILE) $(ML_COMPATIBILITY_FILES)

$(VFILES:.v=.vo) $(VFILES:.v=.vio) $(VFILES:.v=.vi) $(CMOFILES) $(CMXSFILES): $(COQ_VERSION_FILE)

COQFLAGS += $(PROFILE_LTAC_FLAGS) $(WARNINGS_FLAGS)
